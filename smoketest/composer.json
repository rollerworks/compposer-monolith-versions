{
    "name": "rollerworks/smoketest",
    "description": "description",
    "license": "proprietary",
    "authors": [
        {
            "name": "Sebastiaan Stok"
        }
    ],
    "require": {
        "php": ">=8.1",
        "rollerworks/monolith-versions": "dev-main",
        "rollerworks/search": "^2.0",
        "rollerworks/search-doctrine-orm": "^2.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "rollerworks/monolith-versions": true
        }
    },
    "version": "1.0.0",
    "extra": {
        "monolith-versions": {
            "rollerworks-search": {
                "constraint": "^2.0@beta",
                "package": "rollerworks/search-*"
            },
            "symfony": {
                "constraint": "^7.4 || ^8.0",
                "package": "symfony/{console,translation}"
            }
        }
    }
}
